<script src="{{ asset('bundles/Core/Utiles/automation.js') }}"></script>
<script>
    jQuery(document).ready(function() {
        App.init(); // initlayout and core plugins
        $("#change_user_pass_form").validate({
            rules: {
                'fos_user_change_password_form[new][second]':{
                    'equalTo':"#fos_user_change_password_form_new_first"
                }
            }
         });
        $("#modal_change_user_pass").on('hidden.bs.modal',function(){
            $("#change_user_pass_form")[0].reset();
            $("#change_user_pass_form").find('div.has-error').removeClass('has-error');
            $("#change_user_pass_form").find('span.help-block').remove();
        });
        AjaxForm.makeAjax($("#modal_change_user_pass"));
    });


</script>
