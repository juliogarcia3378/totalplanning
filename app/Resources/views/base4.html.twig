        <div class="burger-trigger">
            <button class="menu-trigger">
                <img src="{{ asset('bundles/Core/layout/img/m_toggler.png') }}" alt=""> </button>
            <div class="menu-overlay menu-overlay-bg-transparent">
                <div class="menu-overlay-content">

                    <div class="container-fluid container-lf-space">
                        <!-- BEGIN PAGE BASE CONTENT -->

                        <div class="row">

                            {% block navegation_bundle %}
                                {{ include("@Core/architecture/templates/left_menu.html.twig",{ 'menus':getMenu(),'parentsMenu':getMenuParents()}) }}
                            {% endblock %}


                        </div>


                        <!-- END PAGE BASE CONTENT -->
                    </div>

                </div>
            </div>
            <div class="menu-bg-overlay">
                <button class="menu-close">&times;</button>
            </div>
            <!-- the overlay element -->
        </div>
        <!-- END NAV TRIGGER -->
        <!-- BEGIN LOGO -->
        <div class="page-logo">
            <a href="index.html">
                <img src="{{ asset('bundles/Core/layout/img/logo.png') }}" alt="logo" class="logo-default" /> </a>
        </div>
        <!-- END LOGO -->
        <!-- BEGIN TOP NAVIGATION MENU -->
        <div class="top-menu">
