<!-- BEGIN HEADER & CONTENT DIVIDER -->
<div class="clearfix"> </div>
<!-- END HEADER & CONTENT DIVIDER -->
<!-- BEGIN CONTAINER -->
<!-- BEGIN CONTAINER -->
<div class="page-container">

    <div class="page-content-wrapper">
        <div class="modal fade "  data-backdrop="static" data-keyboard="false" id="modal_change_user_pass" role="basic" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content ajax_form_block">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                        <h4 class="modal-title">Cambiar contraseña</h4>
                    </div>
                    <form  action="{{path('security_crud_user_change_pass') }}"  id="change_user_pass_form" method="POST" class="ajax_form">
                        <div class="modal-body">
                            <div id="change_user_pass_form_container" >
                                {{ render(path('security_crud_user_change_pass'))}}
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn default" data-dismiss="modal">Cerrar</button>
                            <button class="btn green " type="submit"  value="Aceptar">Aceptar</button>
                        </div>
                    </form>
                </div>
                <!-- /.modal-content -->
            </div>
        </div>
        {% block pagecontent %}

            <div class="page-content">
                <div class="page-content-body">
                    {% block breadcrumbs %}
                    {% endblock %}

                    {% block content %}
                        {{ render(path("planeacion_admin_homepage")) }}
                        {#<div class="text-center"><img  class="for_center_in_page_content" width="300px" src="{{ asset('bundles/base/picmark1.png') }}"> </div>#}
                    {% endblock %}
