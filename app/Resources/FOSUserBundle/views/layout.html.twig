<script src="{{ asset('bundles/comun/base/jquery.validate.min.js') }}"></script>
<script src="{{ asset('bundles/comun/base/validation_msgs.js') }}"></script>
{% include '@MySecurity/Usuario/login-soft.html.twig' %}
{#"{{ asset('bundles/mysecurity/images/1.jpg') }}","{{ url('check_valid_user') }}");#}
<script>
    jQuery(document).ready(function() {
        Login.init();
        jQuery.extend(jQuery.validator.messages, {
            equalTo: "Las contrase√±as ingresadas no coinciden."
        });
    });
</script>
{%  if registrationError is defined and registrationError %}
    <script>
        jQuery(document).ready(function() {
            $('#register-btn').click();
        });
    </script>
{% endif %}
<!-- END JAVASCRIPTS -->
{% block javascript %}{% endblock %}
</body>
<!-- END BODY -->
</html>