{% block javascript %}
    <script src="{{ asset('bundles/comun/base/plugins/data-tables/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('bundles/comun/base/plugins/data-tables/DT_bootstrap.js') }}"></script>
