    <script src="{{ asset('bundles/comun/base/scripts/custom/table-ajax.js') }}"></script>
    <script src="{{ asset('bundles/comun/js/ajaxify_form.js') }}"></script>
    <script>
        $("#{{ model.tableId }}"+"_exportar_horario_submit").on('click', function(e){
            var el = $(this);
            var id = e.id;

            $('form.horario_datos_form input.id_row').val(id);
            $('form.horario_datos_form')[0].submit();
        });
    </script>
{% endblock %}



