{% extends '@Comun/architecture/components/CRUD/list.html.twig' %}
{% block form_block %}
    {{ parent() }}
    <div class="modal fade bs-modal-lg"  data-backdrop="static" data-keyboard="false" id="{{ model.tableId }}_modal_materias" role="basic" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content ajax_form_block">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                    <h4 class="modal-title">Registro hist&oacute;rico de materias</h4>
                </div>
                <form  action="{{path(rutas.registroMaterias) }}"  id="{{ model.tableId }}_registro_materias_form" method="POST" class="ajax_form">
                    <div class="modal-body">
                        <div id="{{ model.tableId }}_registro_materias_form_container" class="row" >
                            {#{{ render(path(rutas.new))}}#}
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn default" data-dismiss="modal">Cerrar</button>
                        {#<button class="btn blue " type="submit" value="Aceptar">Aceptar</button>#}
                    </div>
                </form>
            </div>
            <!-- /.modal-content -->
        </div>
    </div>
    <div class="" style="display: none">
        <form class="hoja_datos_form" id="{{ model.tableId }}_exportar_hoja_form" target="_blank" method="get" action="{{ path("planeacion_admin_crud_profesor_expotar_hoja") }}">
            <input  name="id" value="" class="id_row"/>
            {#<input name="iSortCol_0" value="" class="order_column" />#}
            {#<input name="sSortDir_0" value="" class="order_dir" />#}
            <a name="submit" id="{{ model.tableId }}_exportar_hoja_submit"></a>

        </form>
    </div>
{% endblock %}